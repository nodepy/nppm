{
  "name": "nodepy-pm",
  "version": "2.0.0",
  "license": "MIT",
  "description": "The Node.py Package Manager",
  "repository": "https://github.com/nodepy/nodepy-package-manager",
  "keywords": [
    "nodepy",
    "package manager"
  ],
  "categories": [
    "CLI"
  ],
  "authors": [
    "rosensteinniklas@gmail.com"
  ],
  "engines": [
    "nodepy>=2.0.2",
    "python>=2.7.0 || >=3.3.0"
  ],
  "include": [
    ".gitref",
    "lib/**",
    "index.py",
    "LICENSE.txt",
    "README.md"
  ],
  "scripts": {
    "pre-install": "scripts/pre-install.py"
  },
  "bin": {
    "nodepy-pm${py}": "index.py"
  },
  "pip_dependencies": {
    "appdirs": ">=1.4.2",
    "distlib": ">=0.2.4",
    "hammock": ">=0.2.4",
    "requests": ">=2.13.0",
    "six": ">=1.11.0",
    "nr": ">=1.4.8"
  },
  "cfg(dev).dependencies": {
    "nodepy-nosetests": "git+https://github.com/nodepy/nodepy-nosetests.git@v0.0.6"
  }
}